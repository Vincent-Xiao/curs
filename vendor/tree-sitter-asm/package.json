{
  "name": "tree-sitter-asm",
  "version": "0.20.0",
  "description": "Rust grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "asm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tree-sitter/tree-sitter-asm.git"
  },
  "author": "Yijun Yu <yijun.yu@huawei.com> (https://github.com/yijunyu)",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.0"
  },
  "scripts": {
    "test": "tree-sitter test && script/parse-examples",
    "test-windows": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.asm",
      "injection-regex": "asm",
      "file-types": [
        "asm"
      ]
    }
  ]
}
