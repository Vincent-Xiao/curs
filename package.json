{
	"name": "rust-hero",
	"displayName": "Rust Hero",
	"description": "Rust extension that utilizes NLP to enhance the quality of rust code",
	"version": "0.1.0",
	"publisher": "Vincent Xiao,Yijun Yu",
	"repository": "https://github.com/Vincent-Xiao/curs/tree/plugin",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
        "rust",
        "language",
        "grammar",
        "unsafe",
		"lifetime"
    ],
	"license": "Apache-2.0",
	"activationEvents": [
		"onCommand:extension.rust_hero.workspace",
		"onCommand:extension.rust_hero.currentfile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.rust_hero.workspace",
				"title": "Rust Hero: Workspace"
			},
			{
				"command": "extension.rust_hero.currentfile",
				"title": "Rust Hero: Current File"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.13.0",
		"typescript": "^4.7.2"
	}
}
