rm -rf dist/*
python3 setup.py sdist bdist_wheel
if [ "$1" == "main" ]; then
twine upload dist/curious-*
pip3 uninstall curious -y
pip3 install curious
else
pip3 uninstall curious -y
pip3 install dist/curious-*-py3-none-any.whl
curious ~/.cargo/registry/src/github.com-1ecc6299db9ec823/anyhow-1.0.26/src/error.rs
fi
